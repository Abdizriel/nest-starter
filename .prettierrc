  "trailingComma": "all",
  "singleQuote": true,
  "useTabs": false,
  "semi": true,
  "importOrder": ["^@nestjs/(.*)$", "^@xyz/(.*)$", "^[./]"],
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
